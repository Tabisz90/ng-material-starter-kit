{
  "entityRelations": {
    "component": {
      "model": [
        "PROPERTY"
      ],
      "service": [
        "CONSTRUCTOR"
      ],
      "router": [
        "CONSTRUCTOR"
      ],
      "activatedRoute": [
        "CONSTRUCTOR"
      ],
      "queryModel": [
        "PROPERTY"
      ]
    },
    "service": {
      "httpClient": [
        "CONSTRUCTOR"
      ],
      "model": [
        "PARAMETER",
        "RETURN"
      ]
    }
  },
  "external": {
    "httpClient": {
      "type": "HttpClient",
      "module": "@angular/common/http"
    },
    "router": {
      "type": "Router",
      "module": "@angular/router"
    },
    "activatedRoute": {
      "type": "ActivatedRoute",
      "module": "@angular/router"
    }
  },
  "entities": {
    "component": {
      "type": "ANGULAR_COMPONENT",
      "folder": "components/$name$",
      "config": {
        "stylesExtension": "none",
        "viewEncapsulation": "None",
        "selector": "generated",
        "changeDetectionStrategy": "OnPush"
      }
    },
    "model": {
      "type": "DATA_STRUCTURE"
    },
    "queryModel": {
      "folder": "query-models",
      "type": "DATA_STRUCTURE"
    },
    "service": {
      "type": "ANGULAR_SERVICE"
    },
    "interceptor": {
      "type": "ANGULAR_SERVICE"
    },
    "guard": {
      "id": "guard",
      "type": "ANGULAR_GUARD",
      "suffix": "Guard",
      "folder": "guards/$name$"
    },
    "directive": {
      "id": "directive",
      "type": "ANGULAR_DIRECTIVE",
      "suffix": "Directive",
      "folder": "directives/$name$"
    }
  }
}
